{{!--
Render the allOf keyword of a json-schema
@this {object} the json-schema
--}}
{{#if allOf}}
    {{#>json-schema/util/section sections='allOf' title='Must match all of'}}
        <div class="json-schema--subschema-group">
            {{#each allOf}}
                <div class="json-schema--subschema">
                    {{>json-schema/schema .}}
                </div>
            {{/each}}
        </div>
    {{/json-schema/util/section}}
{{/if}}
