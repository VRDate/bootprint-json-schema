{
  "id": "http://some.site.somewhere/entry-schema#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "schema for an fstab entry",
  "type": "object",
  "properties": {
    "age": {
      "type": "integer",
      "minimum": 0
    },
    "bestFriends": {
      "type": "integer",
      "maximum": 5
    },
    "two_to_three": {
      "type": "number",
      "maximum": 3.0,
      "maximumExclusive": true,
      "minimum": 2.0,
      "minimumExclusive": true
    },
    "less_than_three": {
      "type": "number",
      "maximum": 3.0,
      "maximumExclusive": true
    },
    "more_than_two": {
      "type": "number",
      "minimum": 2.0,
      "minimumExclusive": true
    },
    "string_with_range": {
      "type": "string",
      "minimum": 2
    },
    "string_min_length": {
      "type": "string",
      "minLength": 2
    },
    "string_max_length": {
      "type": "string",
      "maxLength": 5
    },
    "string_min_max_length": {
      "type": "string",
      "minLength": 2,
      "maxLength": 5
    }
  }
}